    "url": "https://github.com/dkershner6/amplify-authenticator-react-custom/issues"
  },
  "homepage": "https://github.com/dkershner6/amplify-authenticator-react-custom#readme",
  "devDependencies": {
    "@aws-amplify/auth": "^4.3.11",
    "@aws-amplify/core": "^4.3.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.17.2",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
